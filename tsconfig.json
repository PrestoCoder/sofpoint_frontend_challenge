{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "noEmit": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*.d.ts", "src/**/*.tsx", "src/**/*.ts"]
  }
  